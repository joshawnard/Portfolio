<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>

<h2>Blog Posts</h2>
<%= @posts.each do |post| %>
  <h3><%= post.title %></h3>
  <p><%= post.body %></p>
<% end %>

<h2>Skills</h2>
<%= @skills.each do |skill| %>
  <h3><%= skill.title %></h3>
  <p><%= skill.percent_utilized %></p>
<% end %>
